  "name": "battletris",
  "scripts": {
    "debug": "yarn build && node --inspect dist/src/index.js",
    "debug:game": "yarn build && BATTLETRIS_DEBUG_GAME=true node dist/src/index.js",
    "dev:build": "rm -rf ./dist/node_modules && rm -rf ./dist/src && tsc -p tsconfig.json",
